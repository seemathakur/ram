      <default>
        <earliest>-4h@m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="Env" searchWhenChanged="true">
      <label>Environment</label>
      <choice value="host=ea-app0580 index=kpi">DEV</choice>
      <choice value="host=ea-app0590 index=kpi">TEST</choice>
      <choice value="host=gtsacptas-0408 env=ACPT index=main">ACPT</choice>
      <choice value="host=gtsprodas-1273 env=PROD index=main">PROD-1273</choice>
